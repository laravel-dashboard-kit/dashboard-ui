{
  "name": "laravel-dashboard-kit/dashboard-ui",
  "description": "Dashboard UI package for Laravel Dashboard Kit",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Amr Khaled",
      "email": "i@akhaled.com"
    }
  ],
  "require": {
    "laravel-dashboard-kit/dashboard-abstract": "^1.0",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "illuminate/view": "^6.0|^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
